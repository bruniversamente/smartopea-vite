{
  "kind": "collectionType",
  "collectionName": "processos",
  "info": {
    "singularName": "processo",
    "pluralName": "processos",
    "displayName": "processos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
